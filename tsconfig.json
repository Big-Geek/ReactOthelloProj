{
    "compilerOptions": {
        "target": "es6",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "baseUrl": "src"
    },
    "include": ["src"]
}